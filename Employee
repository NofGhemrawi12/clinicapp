
from Person import Person

class Employee(Person):
    def __init__(self, first, last, phone, email, DoB, address, role, shift):
     Person().__init__(self, first, last, phone, email, DoB, address):
     
     self.role= role
     self.shift= shift
     
        
        
    def name(self):
         return f" my name is {self.name}"
      
    def role(self):
         return f"I have a specialty in {self.role}"
      
    def shift(self):
         return f"my phone nb is {self.shift}"
      
    def email(self):
         return f"my email is {self.email}"        
    
 